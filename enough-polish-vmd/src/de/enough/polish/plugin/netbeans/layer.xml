<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>

    <folder name="Menu" >
        <folder name="Tools">
            <attr name="j2mepolish/Separator1.instance" boolvalue="true"/>
            <folder name="j2mepolish">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="de.enough.polish.netbeans.actions.Bundle"/>
                <file name="de-enough-polish-netbeans-actions-ConvertToPolishProjectAction.instance"/>
            </folder>
        </folder>
    </folder>

    <folder name="Templates">
        <folder name="Project">
            <folder name="J2ME">
                <file name="J2mePolishProjectProject.zip" url="J2mePolishProjectProject.zip">
                    <attr name="SystemFileSystem.localizingBundle" stringvalue="de.enough.polish.netbeans.Bundle"/>
                    <attr name="instantiatingIterator" methodvalue="de.enough.polish.netbeans.J2mePolishProjectTemplateWizardIterator.createIterator"/>
                    <attr name="instantiatingWizardURL" urlvalue="nbresloc:/de/enough/polish/netbeans/J2mePolishProjectTemplateDescription.html"/>
                    <attr name="template" boolvalue="true"/>
                </file>
            </folder>
        </folder>
    </folder>

    <folder name="Navigator">
        <folder name="Panels">
            <folder name="polish-styles">
                <file name="de-enough-polish-plugin-netbeans-navigator-PolishNavigatorPanel.instance"/>
            </folder>
        </folder>
    </folder>
    
    <folder name="WarmUp">
        <file name="de-enough-polish-plugin-netbeans-settings-PolishWarmUp.instance"/>
    </folder>
    
    <folder name="OptionsDialog">
        <folder name="Advanced">
            <file name="de-enough-polish-plugin-netbeans-settings-PolishAdvancedOption.instance"/>
        </folder>
    </folder>
    
    <folder name="j2mepolish">
        <folder name="palette">
            <folder name="backgrounds">
                <file name="de-enough-polish-plugin-netbeans-palette-SimpleBackgroundCreator.instance">
                    <attr name="SystemFileSystem.localizingBundle" stringvalue="de.enough.polish.plugin.netbeans.palette.Bundle"/>
                    <attr name="SystemFileSystem.icon" urlvalue="nbresloc://de/enough/polish/plugin/netbeans/resources/simplebackground.png"/>
                </file>
                <attr boolvalue="true" name="de-enough-polish-plugin-netbeans-palette-SimpleBackgroundCreator.instance/de-enough-polish-plugin-netbeans-palette-ImageBackgroundCreator.instance"/>
                <file name="de-enough-polish-plugin-netbeans-palette-ImageBackgroundCreator.instance">
                    <attr name="SystemFileSystem.localizingBundle" stringvalue="de.enough.polish.plugin.netbeans.palette.Bundle"/>
                    <attr name="SystemFileSystem.icon" urlvalue="nbresloc://de/enough/polish/plugin/netbeans/resources/imagebackground.png"/>
                </file>
            </folder>
            <folder name="borders">
                <file name="de-enough-polish-plugin-netbeans-palette-SimpleBorderCreator.instance">
                    <attr name="SystemFileSystem.localizingBundle" stringvalue="de.enough.polish.plugin.netbeans.palette.Bundle"/>
                    <attr name="SystemFileSystem.icon" urlvalue="nbresloc://de/enough/polish/plugin/netbeans/resources/simpleborder.png"/>
                </file>
            </folder>
        </folder>
    </folder>
    
    <folder name="vmd-midp">
        <folder name="components">
            <file name="de-enough-polish-plugin-netbeans-components-items-BrowserCD.instance"/>
        </folder>
        <folder name="producers">
            <file name="de-enough-polish-plugin-netbeans-components-items-BrowserProducer.instance"/>
        </folder>
    </folder>    

</filesystem>
